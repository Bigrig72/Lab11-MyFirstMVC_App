@model List<MVC_TIME.Models.TimePerson>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Results Page</title>
    <link href="~/StyleSheet.css" rel="stylesheet"/>
</head>
<body>   
        <table>
            <tr>
                <th>Year</th>
                <th>Honor</th>
                <th>Name</th>
                <th>Country</th>
                <th>BirthYear</th>
                <th>DeathYear</th>
                <th>Title</th>
                <th>Category</th>
                <th>Context</th>
            </tr>
        </table>
        @foreach (var item in Model)

        {
            <table>
                <tr>
                    <td>@item.Year</td>
                    <td>@item.Honor</td>
                    <td>@item.Name</td>
                    <td>@item.Country</td>
                    <td>@item.BirthYear</td>
                    <td>@item.DeathYear</td>
                    <td>@item.Title</td>
                    <td>@item.Category</td>
                    <td>@item.Context</td>
                </tr>
            </table>
        } 
</body>
</html>
